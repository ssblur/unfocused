{
  "schemaVersion": 1,
  "id": "unfocused",
  "version": "${version}",
  "name": "Unfocused",
  "description": "Blur's library mod, it just has a lot of useful common code.",
  "authors": [
    "SSBlur"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },
  "license": "CC0-1.0",
  "icon": "assets/unfocused/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.ssblur.unfocused.fabric.UnfocusedModFabric"
    ],
    "client": [
      "com.ssblur.unfocused.fabric.UnfocusedModFabricClient"
    ]
  },
  "mixins": [
    "unfocused.mixins.json",
    "unfocused-common.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "minecraft": "~${minecraft_version}",
    "java": ">=${jvm_version}",
    "fabric-api": ">=${fabric_api_version}",
    "fabric-language-kotlin": ">=${flk_version}"
  },
  "suggests": {}
}
